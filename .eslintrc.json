{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    /*"arrow-parens": [
      "warn",
      "as-needed"
    ],
    "arrow-spacing": "warn",
    "block-spacing": "warn",
    "brace-style": "warn",
    "capitalized-comments": [
      "error",
      "never",
      {
        "ignoreConsecutiveComments": true
      }
    ],
    "camelcase": [
      "error",
      "allways",
      {
        "ignoreImports": true
      }
    ],
    "spaced-comment": "warn",
    "semi": [
      "warn"
    ],*/
    "eol-last": "warn",
    "no-unused-vars": "off",
    "no-trailing-spaces": "warn",
    "semi": "warn",
    "yoda": "warn"
  }
}
